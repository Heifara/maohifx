<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import javafx.scene.input.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Tab?>
<?import com.maohi.software.samples.tableview.json.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<fx:script source="../jsontableview/Controller.js" />
	<center>
		<TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
			<columns>
				<TableColumn text="First Name">
					<cellValueFactory>
						<JSONValueFactory property="firstname" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="Date de naissance">
					<cellValueFactory>
						<JSONValueFactory property="birthdate" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="Enfant">
					<cellValueFactory>
						<JSONValueFactory property="child" />
					</cellValueFactory>
				</TableColumn>
			</columns>
		</TableView>
	</center>
	<fx:script>
		controller = new Controller();
	</fx:script>
</BorderPane>
